Host *
    HashKnownHosts no
    IdentityFile ~/.ssh/id_rsa

Host servern
  HostName servern
  ForwardAgent yes
  User cada
  ProxyCommand ssh -i ~/.ssh/id_rsa -W %h:%p root@carlb.duckdns.org
  LocalForward 8888 192.168.1.2:8888
  LocalForward 8080 192.168.1.1:80  
  LocalForward 9091 192.168.1.2:9091
